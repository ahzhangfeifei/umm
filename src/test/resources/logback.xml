<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<layout class="ch.qos.logback.classic.PatternLayout">
			<pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} [%thread] %-5level %logger{36} -%msg%n</pattern>
		</layout>
	</appender>
	<appender name="ASYNC" class="ch.qos.logback.classic.AsyncAppender">
		<discardingThreshold>0</discardingThreshold>
		<queueSize>512</queueSize>
		<appender-ref ref="STDOUT"/>
	</appender>

	<logger name="aop.request.body" level="trace" additivity="false">
		<appender-ref ref="ASYNC"/>
	</logger>

	<logger name="com.umm.tracer">
		<level value="info"/>
	</logger>
	<!-- logger name="com.umm.zkconfig">
        <level value="trace"/>
    </logger>
    <logger name="com.umm.abt">
        <level value="trace"/>
    </logger>
    <logger name="com.umm.vla.base">
        <level value="trace"/>
    </logger>
    <logger name="com.umm.adapter">
        <level value="trace"/>
    </logger-->
	<logger name="com.umm.operation.platform.tsg">
		<level value="info"/>
	</logger>
	<logger name="org.springframework.transaction">
		<level value="info"/>
	</logger>
	<logger name="org.apache.curator">
		<level value="warn"/>
	</logger>
	<logger name="org.apache.zookeeper">
		<level value="warn"/>
	</logger>
	<!-- logger name="org.mybatis">
        <level value="trace"/>
    </logger>
    <logger name="org.apache.http">
        <level value="trace"/>
    </logger>
    <logger name="org.apache.ibatis">
        <level value="trace"/>
    </logger>
    <logger name="tk.mybatis">
        <level value="trace"/>
    </logger-->
	<logger name="com.umm">
		<level value="debug"/>
	</logger>
	<logger name="com.umm.abt.mapper">
		<level value="debug"/>
	</logger>
	<root level="WARN">
		<appender-ref ref="ASYNC"/>
	</root>


</configuration>